{
  "display_information": {
    "name": "Ragstar Integration",
    "description": "Connect Slack with your Ragstar application.",
    "background_color": "#2F6EAF"
  },
  "features": {
    "bot_user": {
      "display_name": "ragstar",
      "always_online": true
    },
    "slash_commands": [
      {
        "command": "/ragstar",
        "url": "https://your-ragstar-api.com/slack/commands",
        "description": "Interact with Ragstar.",
        "usage_hint": "[your query or command]",
        "should_escape": false
      }
    ]
  },
  "oauth_config": {
    "scopes": {
      "bot": [
        "commands",
        "im:history",
        "app_mentions:read",
        "chat:write",
        "chat:write.public",
        "channels:history",
        "files:read",
        "files:write"
      ]
    }
  },
  "settings": {
    "interactivity": {
      "is_enabled": true,
      "request_url": "https://your-ragstar-api.com/slack/events"
    },
    "org_deploy_enabled": false,
    "socket_mode_enabled": false,
    "token_rotation_enabled": false,
    "event_subscriptions": {
      "request_url": "https://your-ragstar-api.com/slack/events",
      "bot_events": ["app_mention", "message.im"]
    }
  }
}
