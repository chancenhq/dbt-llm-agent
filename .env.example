# OpenAI API key
OPENAI_API_KEY=your_openai_api_key

# OpenAI model to use
OPENAI_MODEL=gpt-4o-mini
OPENAI_EMBEDDING_MODEL=text-embedding-3-small

# Temperature for text generation (0-1)
TEMPERATURE=0.0

# PostgreSQL connection URI
POSTGRES_URI=postgresql://postgres:postgres@localhost:5432/dbt_llm_agent

# Path to store the vector database
VECTOR_DB_PATH=./data/vector_db

# Path to the dbt project
DBT_PROJECT_PATH=/path/to/your/dbt/project

# Slack tokens for integration
SLACK_BOT_TOKEN=xoxb-your-bot-token
SLACK_APP_TOKEN=xapp-your-app-token
SLACK_SIGNING_SECRET=your-signing-secret

# Logging configuration
DBT_LLM_AGENT_LOG_LEVEL=INFO
DBT_LLM_AGENT_LOG_FILE=~/.dbt-llm-agent/logs/dbt-llm-agent.log 